{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@users/*": [
        "src/users/*"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@auth/*": [
        "src/auth/*"
      ],
      "@stripe/*": [
        "src/stripe/*"
      ],
      "@success/*":["src/success/*"],
    }
  }
}